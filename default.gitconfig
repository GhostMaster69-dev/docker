[safe]
        directory = *
[filter "lfs"]
        clean = git-lfs clean -- %f
        smudge = git-lfs smudge -- %f
        process = git-lfs filter-process
        required = true
[color]
        ui = true
[http]
	postBuffer = 1024M
	maxRequestBuffer = 512M
[ssh]
	postBuffer = 1024M
	maxRequestBuffer = 512M
[core]
	compression = 9
[pack]
	windowMemory = 100m
	packSizeLimit = 100m
